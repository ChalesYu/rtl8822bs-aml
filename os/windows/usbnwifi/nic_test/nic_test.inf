;
; nic_test.inf
;

[Version]
Signature="$WINDOWS NT$"
Class=Sample ; TODO: edit Class
ClassGuid={78A1C341-4539-11d3-B88D-00C04FAD5171} ; TODO: edit ClassGuid
Provider=%ManufacturerName%
CatalogFile=nic_test.cat
DriverVer= ; TODO: set DriverVer in stampinf property pages

[DestinationDirs]
DefaultDestDir = 12
nic_test_Device_CoInstaller_CopyFiles = 11

; ================= Class section =====================

[ClassInstall32]
Addreg=SampleClassReg

[SampleClassReg]
HKR,,,0,%ClassName%
HKR,,Icon,,-5

[SourceDisksNames]
1 = %DiskName%,,,""

[SourceDisksFiles]
nic_test.sys  = 1,,
WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll=1 ; make sure the number matches with SourceDisksNames

;*****************************************
; Install Section
;*****************************************

[Manufacturer]
%ManufacturerName%=Standard,NT$ARCH$

[Standard.NT$ARCH$]
%nic_test.DeviceDesc%=nic_test_Device, Root\nic_test ; TODO: edit hw-id

[nic_test_Device.NT]
CopyFiles=Drivers_Dir

[Drivers_Dir]
nic_test.sys

;-------------- Service installation
[nic_test_Device.NT.Services]
AddService = nic_test,%SPSVCINST_ASSOCSERVICE%, nic_test_Service_Inst

; -------------- nic_test driver install sections
[nic_test_Service_Inst]
DisplayName    = %nic_test.SVCDESC%
ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\nic_test.sys

;
;--- nic_test_Device Coinstaller installation ------
;

[nic_test_Device.NT.CoInstallers]
AddReg=nic_test_Device_CoInstaller_AddReg
CopyFiles=nic_test_Device_CoInstaller_CopyFiles

[nic_test_Device_CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll,WdfCoInstaller"

[nic_test_Device_CoInstaller_CopyFiles]
WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll

[nic_test_Device.NT.Wdf]
KmdfService =  nic_test, nic_test_wdfsect
[nic_test_wdfsect]
KmdfLibraryVersion = $KMDFVERSION$

[Strings]
SPSVCINST_ASSOCSERVICE= 0x00000002
ManufacturerName="<Your manufacturer name>" ;TODO: Replace with your manufacturer name
ClassName="Samples" ; TODO: edit ClassName
DiskName = "nic_test Installation Disk"
nic_test.DeviceDesc = "nic_test Device"
nic_test.SVCDESC = "nic_test Service"
