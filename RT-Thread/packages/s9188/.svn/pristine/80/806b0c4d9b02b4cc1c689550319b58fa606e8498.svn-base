	for (i = 0; i < NUM_SUPPORTED_OIDS; i++) {
		*((PNDIS_OID) pvQryBuf + i) = arWlanOidReqTable[i].rOid;
	}

	return NDIS_STATUS_SUCCESS;
}	/* reqQuerySupportedList */

